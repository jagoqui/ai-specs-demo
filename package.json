{
  "name": "ai-specs-demo",
  "homepage": "https://ai-specs-demo.netlify.app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "A demo application for showcasing AI agents usage, built with React and Vite, following clean code best practices and hexagonal architecture.",
  "author": {
    "name": "Jaidiver Gómez Quintero",
    "email": "jagoqui.gomez@gmail.com"
  },
  "maintainers": [
    {
      "name": "Jaidiver Gómez Quintero",
      "email": "jagoqui.gomez@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagoqui/ai-specs-demo.git"
  },
  "bugs": {
    "url": "https://github.com/jagoqui/ai-specs-demo/issues"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "tailwindcss",
    "clean-code",
    "hexagonal-architecture",
    "ai-specs",
    "tanstack"
  ],
  "publisher": "jagoqui",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "dev": "vite",
    "staging": "vite --mode staging",
    "build": "tsc -b && vite build",
    "check-types": "tsc --noemit",
    "preview": "vite preview",
    "test": "vitest run",
    "test:staged": "vitest --changed --run",
    "test:watch": "vitest",
    "test:single": "node scripts/vitest-single-coverage.js",
    "lint": "eslint src --no-warn-ignored --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --no-warn-ignored --report-unused-disable-directives --fix --max-warnings 0",
    "format": "prettier --write src --ignore-unknown",
    "markdownlint": "markdownlint-cli2 --fix 'README.md' 'ai-specs/**/*.{md,mdc}' 'documentation/**/*.md'",
    "sonar-scanner:local": "docker rm -f sonar-scanner-cli; node ./scripts/sonar-scanner.js -local",
    "sonarqube:image:build": "docker rm -f sonarqube; docker run -d --name sonarqube -e SONAR_ES_BOOTSTRAP_CHECKS_DISABLE=true -p 9000:9000 sonarqube:latest",
    "sonarqube:image:start": "docker start sonarqube",
    "sonarqube:image:stop": "docker stop sonarqube",
    "snyk:auth": "npx snyk auth",
    "snyk:dependencies": "node --env-file=environments/.env ./scripts/snyk.js",
    "snyk:static": "node --env-file=environments/.env ./scripts/snyk.js -static",
    "snyk:monitor": "node --env-file=environments/.env ./scripts/snyk.js -monitor",
    "snyk:runner": "npm run snyk:static; npm run snyk:dependencies; npm run snyk:monitor",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.4.0",
    "@tanstack/react-devtools": "^0.9.0",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.145.7",
    "@tanstack/react-router-devtools": "^1.145.7",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "install": "^0.13.0",
    "ky": "^1.14.2",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@react-scan/vite-plugin-react-scan": "^0.1.8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/eslint-plugin": "^1.6.5",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-path": "^2.0.3",
    "eslint-plugin-paths": "^1.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.20.0",
    "prettier": "^3.7.4",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16",
    "vitest-sonar-reporter": "^3.0.0"
  }
}
